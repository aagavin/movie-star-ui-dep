{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended", "tslint-react", "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "interface-name": [true, "never-prefix"],
        "jsx-boolean-value": [true, "never"],
        "object-literal-sort-keys": [false]
    },
    "rulesDirectory": []
}