{
  "name": "movie-star-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^1.3.0",
    "@capacitor/core": "1.3.0",
    "@ionic/react": "^4.11.0",
    "@ionic/react-router": "^4.11.0",
    "firebase": "^7.2.3",
    "ionicons": "^4.6.3",
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-dom": "^16.10.2",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.2.0",
    "react-with-firebase-auth": "^1.0.4",
    "typescript": "3.6.4",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "@capacitor/cli": "1.3.0",
    "@testing-library/react": "^9.3.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.22",
    "@types/node": "12.12.8",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.3",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.0",
    "fetch-mock": "^7.7.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-android": "npm run build; npx cap copy; cd ./android; gradle app:assemble",
    "deploy": "ionic build; firebase deploy",
    "test": "react-scripts test",
    "test-local": "npm run test -- --coverage --watchAll=true",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.tsx",
      "!./src/index.tsx",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60,
        "branches": 55,
        "functions": 50,
        "lines": 60
      }
    },
    "coverageReporters": [
      "text",
      "json"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "An Ionic project"
}
