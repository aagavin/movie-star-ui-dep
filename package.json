    "@ionic/react-test-utils": "0.0.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@types/fetch-mock": "^7.3.2",
    "@types/jest": "^25.1.3",
    "@types/node": "13.9.1",
    "@types/react": "^16.9.23",
