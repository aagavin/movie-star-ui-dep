{
  "name": "movie-star-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^2.0.2",
    "@capacitor/core": "2.0.2",
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.7",
    "firebase": "^7.14.2",
    "ionicons": "^5.0.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.1",
    "react-visibility-sensor": "^5.1.1",
    "react-with-firebase-auth": "^1.3.2",
    "typescript": "3.8.3",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "@capacitor/cli": "2.0.2",
    "@ionic/react-test-utils": "0.0.3",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.1.0",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/fetch-mock": "^7.3.2",
    "@types/jest": "^25.2.1",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/node": "13.13.5",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.32.0",
    "eslint": "^6.8.0",
    "fetch-mock": "^9.5.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^2.1.1",
    "ts-jest": "^25.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-android": "npm run build; npx cap copy; cd ./android; gradle app:assemble",
    "deploy": "ionic build; firebase deploy",
    "test": "npm run test:integration",
    "test:unit": "react-scripts test",
    "test-local": "npm run test -- --coverage --watchAll=true",
    "test:integration": "jest -c ./src/__tests__/integration/jest.config.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-explicit-any": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.tsx",
      "!./src/index.tsx",
      "!<rootDir>/node_modules/"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "An Ionic project"
}
